public class program10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		int tam = 0;
		
		
		System.out.println("Quantos alunos serao digitados? ");
		tam = sc.nextInt();
		sc.close();
	}
}
